{:deps true

 :builds
 {:app
  {:target :browser
   :output-dir "public/js"
   :asset-path "/js"
   :modules {:main {:init-fn bashketball-editor-ui.core/init!}}
   :devtools {:http-root "public"
              :http-port 3000
              :preloads [devtools.preload]}}

  :test
  {:target :node-test
   :output-to "out/node-tests.js"
   :prepend-js "require('global-jsdom/register');"
   :autorun true
   :deps {:aliases [:test]}
   :ns-regexp "bashketball-editor-ui\\..*-test$"}}}
