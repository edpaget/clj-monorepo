entity User {
    role: String,
    level: Long,
    status: String,
    age: Long,
    score: Long,
    department: String,
    clearance: Long,
    tenure: Long,
    karma: Long,
    warnings: Long,
    reputation: Long,
    verified: Bool,
    region: String,
    "restricted-flag": String,
    "account-age": Long,
    "trust-score": Long,
    subscription: String,
    trial: Bool,
    // Quantifier benchmark attributes
    roles: Set<String>,
    "active-flags": Set<String>,
    // Count benchmark attributes
    "user-ids": Set<String>,
    "active-user-ids": Set<String>,
    "org-member-ids": Set<String>,
    active: Bool,
};

entity Resource {};

action "access" appliesTo {
    principal: User,
    resource: Resource,
};
